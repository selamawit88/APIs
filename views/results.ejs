 <h1>Results Page!!!</h1>
 
<% if (data.Response === "False") { %>
 
<p><%= data.Error %> Please try refining your search term.</p>
 
<% } else { %>
 
<ul>
    <% data["Search"].forEach(function(movie) { %>
    <li>
        <strong>
            <%= movie["Title"] %>
        </strong>
        - <%=movie["Year"]%>
    </li>
    <% }) %>
</ul>
 
<% } %>
 
<a href="/">Search Again!</a>